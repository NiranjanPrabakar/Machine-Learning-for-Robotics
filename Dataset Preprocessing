        "# Create sequences of 12 months for each subdivision\n",
        "X, y = [], []\n",
        "for i in range(len(df) - 12):  # We need at least 12 months of data for each sample\n",
        "    X.append(df[rainfall_cols].iloc[i:i+12].values)  # 12 months of data as sequence\n",
        "    y.append(df[rainfall_cols].iloc[i+12].values)  # Next year's data (12 months)\n",
        "\n",
        "X = np.array(X)\n",
        "y = np.array(y)"
